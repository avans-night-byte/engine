<?xml version="1.1" encoding="UTF-8"?>
<obj:objectList xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="Objects  ../../Schemas/objects.xsd
                                    Common  ../../Schemas/common.xsd"
                xmlns:obj="Objects"
                xmlns:common="Common">

    <object>
        <name>bullet</name>
        <objectType>resource</objectType>
        <components>
            <component>
                <componentName>PhysicsComponent</componentName>
                <physicsComponent>
                    <common:position>
                        <x>200</x>
                        <y>200</y>
                    </common:position>
                    <friction>1</friction>
                    <bodyType>Dynamic</bodyType>
                    <bodyShape>
                        <box>
                            <width>30</width>
                            <height>13</height>
                        </box>
                    </bodyShape>
                    <isBullet>true</isBullet>
                    <isSensor>false</isSensor>
                    <isEnabled>false</isEnabled>
                </physicsComponent>
            </component>

            <component>
                <componentName>BulletComponent</componentName>
                <bulletComponent/>
            </component>

            <component>
                <componentName>TransformComponent</componentName>
                <transformComponent>
                    <common:position>
                        <x>200</x>
                        <y>200</y>
                    </common:position>
                </transformComponent>
            </component>

            <component>
                <componentName>RenderComponent</componentName>
                <renderComponent>
                    <spriteId>bullet</spriteId>
                    <size>
                        <width>60</width>
                        <height>25</height>
                    </size>
                    <offset>
                        <width>0</width>
                        <height>0</height>
                    </offset>
                </renderComponent>
            </component>
        </components>
    </object>

    <object>
        <name>crate</name>
        <objectType>object</objectType>
        <components>
            <component>
                <componentName>PhysicsComponent</componentName>
                <physicsComponent>
                    <common:position>
                        <x>200</x>
                        <y>200</y>
                    </common:position>
                    <friction>1</friction>
                    <bodyType>Dynamic</bodyType>
                    <bodyShape>
                        <box>
                            <width>64</width>
                            <height>64</height>
                        </box>
                    </bodyShape>
                    <isBullet>false</isBullet>
                    <isSensor>false</isSensor>
                    <isEnabled>false</isEnabled>
                </physicsComponent>
            </component>

            <component>
                <componentName>TransformComponent</componentName>
                <transformComponent>
                    <common:position>
                        <x>200</x>
                        <y>200</y>
                    </common:position>
                </transformComponent>
            </component>

            <component>
                <componentName>RenderComponent</componentName>
                <renderComponent>
                    <spriteId>crate</spriteId>
                    <size>
                        <width>128</width>
                        <height>128</height>
                    </size>
                    <offset>
                        <width>0</width>
                        <height>0</height>
                    </offset>
                </renderComponent>
            </component>
        </components>
    </object>

    <object>
        <name>boar</name>
        <objectType>object</objectType>
        <components>
            <component>
                <componentName>PhysicsComponent</componentName>
                <physicsComponent>
                    <common:position>
                        <x>200</x>
                        <y>200</y>
                    </common:position>
                    <friction>1</friction>
                    <bodyType>Dynamic</bodyType>
                    <bodyShape>
                        <box>
                            <width>64</width>
                            <height>64</height>
                        </box>
                    </bodyShape>
                    <isBullet>false</isBullet>
                    <isSensor>false</isSensor>
                    <isEnabled>false</isEnabled>
                </physicsComponent>
            </component>

            <component>
                <componentName>TransformComponent</componentName>
                <transformComponent>
                    <common:position>
                        <x>200</x>
                        <y>200</y>
                    </common:position>
                </transformComponent>
            </component>

            <component>
                <componentName>RenderComponent</componentName>
                <renderComponent>
                    <spriteId>boar</spriteId>
                    <size>
                        <width>128</width>
                        <height>128</height>
                    </size>
                    <offset>
                        <width>0</width>
                        <height>0</height>
                    </offset>
                </renderComponent>
            </component>
        </components>
    </object>

    <common:preloadResources>
        <resource>boar</resource>
        <resource>crate</resource>
        <resource>bullet</resource>
        <resource>zombie</resource>
    </common:preloadResources>
</obj:objectList>