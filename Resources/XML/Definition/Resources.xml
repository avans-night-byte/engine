    </sprites>
    <sounds>
        <sound>
            <name>oof</name>
            <path>Sounds/oof.wav</path>
        </sound>
    </sounds>
    <music>
        <music>
            <name>mainmenu_music</name>
            <path>Sounds/background.wav</path>
        </music>
    </music>
    <scenes/>
    <levels/>
