<?xml version="1.1" encoding="UTF-8"?>
<gr:resources
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="GameResources  ../Schemas/resources.xsd"
        xmlns:common="Common"
        xmlns:gr="GameResources">
    <basePath>../../Resources/</basePath>
    <textures>
        <texture>
            <name>mainmenu_background</name>
            <path>Sprites/background.png</path>
        </texture>
        <texture>
            <name>github_logo</name>
            <path>Sprites/github_button.png</path>
        </texture>
        <texture>
            <name>sascha</name>
            <path>Sprites/egg.png</path>
        </texture>
        <texture>
            <name>minion</name>
            <path>Sprites/booty-bum.gif</path>
        </texture>
        <texture>
            <name>nightbyte_logo</name>
            <path>Sprites/NightByte.png</path>
        </texture>
    </textures>
    <sprites>
        <sprite>
            <name>character</name>
            <path>Sprites/character.png</path>
            <common:size>
                <width>1</width>
                <height>1</height>
            </common:size>
        </sprite>
    </sprites>
    <sounds>
        <sound>
            <name>bonk</name>
            <path>Sounds/bonk.wav</path>
        </sound>
        <sound>
            <name>oof</name>
            <path>Sounds/oof.wav</path>
        </sound>
    </sounds>
    <music>
        <music>
            <name>mainmenu_music</name>
            <path>Sounds/background.wav</path>
        </music>
        <music>
            <name>background_2</name>
            <path>Sounds/background_2.wav</path>
        </music>
    </music>
    <scenes>
        <scene>
            <name>MainMenu</name>
            <path>XML/Definition/MainMenu.xml</path>
        </scene>
        <scene>
            <name>Credits</name>
            <path>XML/Definition/Credits.xml</path>
        </scene>
    </scenes>
    <levels/>
</gr:resources>