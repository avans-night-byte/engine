<?xml version="1.1" encoding="UTF-8"?>
<gr:resources
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="GameResources  ../Schemas/resources.xsd"
        xmlns:common="Common"
        xmlns:gr="GameResources">
    <basePath>../../Resources/</basePath>
    <textures>
        <texture>
            <name>mainmenu_background</name>
            <path>Sprites/background.png</path>
        </texture>
    </textures>
    <sprites>
        <sprite>
            <name>character</name>
            <path>Sprites/character.png</path>
            <common:size>
                <width>1</width>
                <height>1</height>
            </common:size>
        </sprite>
    </sprites>
    <sounds>
        <sound>
            <name>oof</name>
            <path>Sounds/oof.wav</path>
        </sound>
    </sounds>
    <music>
        <music>
            <name>background_mainmenu</name>
            <path>Sounds/background.wav</path>
        </music>
    </music>
    <scenes/>
    <levels/>
</gr:resources>


