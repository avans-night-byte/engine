<?xml version="1.1" encoding="UTF-8"?>
<!-- Elements start with X to prevent duplicate class names when generating xsd files into cxx files -->
<lr:Level xmlns:lr="LevelResources">

    <Object>
        <Name>Character</Name>

        <Components>
            <WorldPositionComponent>
                <position>
                    <x>1</x>
                    <y>2</y>
                </position>
            </WorldPositionComponent>

            <PhysicsComponent>

            </PhysicsComponent>
        </Components>

        <XComponent name="CharacterComponent">
            <XParam Name="Health">80</XParam>
        </XComponent>

        <XComponent name="PhysicsComponent">
            <XParam Name="friction">1</XParam>
            <XParam Name="bouyancy">0.2</XParam>
        </XComponent>
    </Object>

    <Object name="Crate">
        <XComponent name="PhysicsComponent">
            <XParam Name="friction">100</XParam>
        </XComponent>
    </Object>
</lr:Level>