<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:cmn="Common"
targetNamespace="Sentry">
    <xs:import schemaLocation="common.xsd"
               namespace="Common"/>

    <xs:element name="sentry">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="className" type="xs:string"/>

                <xs:element ref="cmn:resources"/>
                <xs:element ref="cmn:collider"/>
                <xs:element ref="cmn:events"/>

            </xs:sequence>
        </xs:complexType>
    </xs:element>


</xs:schema>